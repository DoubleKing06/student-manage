<form name="studentPingjiangUpdateForm" action="/studentPingjiang/save" method="POST" data-resultType="JSON">
	<table class="table table-bordered table-striped formLayout m0">
		<tr>
			<td>id:</td>
			<td><input type="text" name="id" value="$!{studentPingjiang.id}" placeholder="id" readonly/></td>
			<td>学生编号:</td>
			<td><input type="text" name="studentId" value="$!{studentPingjiang.studentId}" placeholder="学生编号"/></td>
		</tr>
		<tr>
			<td>学期:</td>
			<td><input type="text" name="xueqi" value="$!{studentPingjiang.xueqi}" placeholder="学期"/></td>
			<td>奖学金:</td>
			<td><input type="text" name="jiangxuejin" value="$!{studentPingjiang.jiangxuejin}" placeholder="奖学金"/></td>
		</tr>
		<tr>
			<td>单项奖学金:</td>
			<td><input type="text" name="danxiangjiangxuejin" value="$!{studentPingjiang.danxiangjiangxuejin}" placeholder="单项奖学金"/></td>
			<td>学优:</td>
			<td><input type="text" name="xueyou" value="$!{studentPingjiang.xueyou}" placeholder="学优"/></td>
		</tr>
		<tr>
			<td>团优:</td>
			<td><input type="text" name="tuanyou" value="$!{studentPingjiang.tuanyou}" placeholder="团优"/></td>
			<td>优秀大学毕业生:</td>
			<td><input type="text" name="yxdxbys" value="$!{studentPingjiang.yxdxbys}" placeholder="优秀大学毕业生"/></td>
		</tr>
		<tr>
			<td>党校学习:</td>
			<td><input type="text" name="dxxx" value="$!{studentPingjiang.dxxx}" placeholder="党校学习"/></td>
			<td>更新时间:</td>
			<td><input type="text" name="updateTime" value="$!{studentPingjiang.updateTime}" placeholder="更新时间"/></td>
		</tr>
	</table>
</form>
<div class="bottomBar">
	<button class="btn btn-primary" onclick="$('form[name=studentPingjiangUpdateForm]').submit()">保存更改</button>
</div>